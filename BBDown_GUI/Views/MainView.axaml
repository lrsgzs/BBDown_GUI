<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:vm="using:BBDown_GUI.ViewModels"
             xmlns:views="clr-namespace:BBDown_GUI.Views"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="BBDown_GUI.Views.MainView"
             x:DataType="views:MainView"
             d:DataContext="{d:DesignInstance views:MainView}"
             Unloaded="OnUnloaded">
    <TextBox Text="{Binding ViewModel.Config.Test}" />
</UserControl>
